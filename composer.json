{
    "name": "zalehy/ws-apiv2-sdk-php",
    "version": "2.0.0",
    "description": "Wheel Fitment API PHP client SDK",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "WheelSizeApiClient\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "WheelSizeApiClient\\" : "test/" }
    }
}
